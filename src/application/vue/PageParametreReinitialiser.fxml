<?xml version="1.0" encoding="UTF-8"?>

<!-- grid.fxml -->

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="rootPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="720.0" prefWidth="1030.0"
	style="-fx-background-color: white;"
	xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.controlleur.Controlleur">
	<top>
		<GridPane BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES"
					minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints maxHeight="96.0" minHeight="10.0"
					prefHeight="88.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="60.0" minHeight="10.0"
					prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints maxHeight="28.0" minHeight="0.0"
					prefHeight="0.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<GridPane style="-fx-background-color: #e6e9f0;">
					<children>
						<Pane prefHeight="200.0" prefWidth="200.0"
							style="-fx-background-color: #354B85;" />
						<Pane prefHeight="200.0" prefWidth="200.0"
							style="-fx-background-color: #354B85;" GridPane.columnIndex="1" />
						<Pane prefHeight="48.0" prefWidth="200.0"
							style="-fx-background-color: #354B85;" GridPane.columnIndex="2">
							<children>
								<Label fx:id="labelNomEtudiant" alignment="CENTER_RIGHT"
									layoutX="14.0" layoutY="15.0" prefHeight="20.0"
									prefWidth="485.0" text="Label" textFill="#e6e9f0">
									<font>
										<Font name="System Bold" size="14.0" />
									</font>
								</Label>
							</children>
						</Pane>
						<Button fx:id="boutonNotes"
							maxHeight="1.7976931348623157E308" maxWidth="-Infinity"
							mnemonicParsing="false" onAction="#changerSceneNotes"
							prefHeight="40.0" prefWidth="515.0"
							style="-fx-background-color: #e6e9f0;" text="Mes notes"
							textFill="#354b85" GridPane.rowIndex="1" />
						<Button fx:id="boutonParametreImporter" alignment="CENTER"
							mnemonicParsing="false" prefHeight="37.0" prefWidth="515.0"
							style="-fx-background-color: #354B85;" text="Paramètres"
							textFill="#e6e9f0" GridPane.columnIndex="2" GridPane.rowIndex="1"
							GridPane.valignment="BOTTOM" />
					</children>
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="515.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="0.0" prefWidth="0.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="515.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="48.0"
							vgrow="SOMETIMES" />
						<RowConstraints minHeight="10.0" prefHeight="40.0"
							vgrow="SOMETIMES" />
					</rowConstraints>
				</GridPane>
				<GridPane style="-fx-background-color: #e6e9f0;"
					GridPane.rowIndex="1">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="203.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="203.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="203.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="203.0" />
						<ColumnConstraints hgrow="SOMETIMES"
							minWidth="10.0" prefWidth="203.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints maxHeight="-Infinity"
							minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Button fx:id="boutonParametreImporter2"
							mnemonicParsing="false" onAction="#changerSceneParametre"
							prefHeight="75.0" prefWidth="258.0"
							style="-fx-background-color: #e6e9f0;"
							text="Importer des paramètres" textFill="#354b85" />
						<Button fx:id="boutonParametrePartager"
							mnemonicParsing="false" onAction="#changerSceneParametrePartager"
							prefHeight="125.0" prefWidth="258.0"
							style="-fx-background-color: #e6e9f0;"
							text="Partager les paramètres" textFill="#354b85"
							GridPane.columnIndex="1" />
						<Button fx:id="boutonParametreModifier"
							mnemonicParsing="false" onAction="#changerSceneModifier"
							prefHeight="133.0" prefWidth="257.0"
							style="-fx-background-color: #e6e9f0;"
							text="Modifier mon identité" textFill="#354b85"
							GridPane.columnIndex="4" />
						<Button fx:id="boutonParametreReinitialiser"
							mnemonicParsing="false" onAction="#changerSceneParametreReiniti"
							prefHeight="97.0" prefWidth="257.0"
							style="-fx-background-color: #354B85;"
							text="Réinitialiser mes données" textFill="#e6e9f0"
							GridPane.columnIndex="2" />
						<Button fx:id="boutonRessources" mnemonicParsing="false"
							onAction="#changerSceneEnseignements" prefHeight="125.0"
							prefWidth="258.0" style="-fx-background-color: #e6e9f0;"
							text="Les ressources" textFill="#354b85" GridPane.columnIndex="3" />
					</children>
				</GridPane>
			</children>
		</GridPane>
	</top>
</BorderPane>
